{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.2.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*@dev",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.1.*",
        "symfony/swiftmailer-bundle": "2.2.*",
        "monolog/monolog": "1.3.1",
        "symfony/monolog-bundle": "2.2.*",
        "sensio/distribution-bundle": "2.2.*",
        "sensio/framework-extra-bundle": "2.2.*",
        "sensio/generator-bundle": "2.2.*",
        "jms/security-extra-bundle": "1.4.*",
        "jms/di-extra-bundle": "1.3.*",
        "twitter/bootstrap": "2.2.2",
        "leafo/lessphp": "v0.3.8",
        "knplabs/knp-menu-bundle": "2.0.*@dev",
        "hwi/oauth-bundle": "dev-master",
        "friendsofsymfony/user-bundle": "1.3.1",
        "jms/translation-bundle": "1.1.*@dev",
        "white-october/pagerfanta-bundle": "dev-master",
        "knplabs/github-api": "*",
        "knplabs/knp-time-bundle": "1.1",
        "stof/doctrine-extensions-bundle": "dev-master",
        "sonata-project/formatter-bundle": "dev-master",
        "sonata-project/intl-bundle": "dev-master"
    },
    "minimum-stability": "dev",
    "scripts": {
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ]
    },
    "repositories": [
                {
                    "type": "package",
                    "package": {
                                "name": "twitter/bootstrap",
                                "version": "2.2.2",
                                "source": {
                                            "type": "git",
                                            "url": "https://github.com/twitter/bootstrap",
                                            "reference": "v2.2.2"
                                }
                    }
                 }
    ],

    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
